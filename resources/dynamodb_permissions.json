{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"dynamodb:PutItem",
				"dynamodb:CreateTable",
				"dynamodb:DeleteItem",
				"dynamodb:Scan",
				"dynamodb:GetItem",
				"dynamodb:DescribeTable"
			],
			"Resource": "arn:aws:dynamodb:us-east-1:616778682758:table/*"
		}
	]
}